                function settime()
                {
                     var start = document.getElementById("from_timeid").value;
                    var end = document.getElementById("to_timeid").value;
                    
                    if(start && end) {
                        start = document.getElementById("from_timeid").value; //to update time value in each input bar
                        end = document.getElementById("to_timeid").value; //to update time value in each input bar
                        
                        start = start.split(":");
                        end = end.split(":");
                        var startDate = new Date(0, 0, 0, start[0], start[1], 0);
                        var endDate = new Date(0, 0, 0, end[0], end[1], 0);
                        
                        var diff = endDate.getTime() - startDate.getTime();
                        if(diff<0)
                        {
                            alert("Choose the valid time");
                             document.getElementById("from_timeid").value="";
                            document.getElementById("to_timeid").value="";
                        }
                        var hours = Math.floor(diff / 1000 / 60 / 60);
                        var minutes = Math.floor(diff / 1000 / 60);
                        
                        if(minutes>60)
                        {
                            document.getElementById("from_timeid").value="";
                            document.getElementById("to_timeid").value="";
                            alert("Permission is allowed only for maximum of 60 Minutes\n\n You Trying to apply for "+minutes+" Minutes");
                            
                        }
                    }
                    
                    
                    
                 }
                
                function time_valid(){
                    var from_date=document.getElementById("from_dateid").value;
                    var from_session=document.getElementById("from_session").value;
                    var to_date=document.getElementById("to_dateid").value;
                    var to_session=document.getElementById("to_session").value;
                    var cdate=new Date();
                    cdate.setDate(cdate.getDate() -1);
                    cdate=cdate.toISOString().slice(0, 10);
                    if(from_date<cdate)
                    {
                        alert("Leave can be applied within 2 days");
                        document.getElementById("from_dateid").value="";
                        location.reload(); 
                    }
                    var m=0;
                    if(from_date!='' && from_session!='' && to_date!='' && to_session!='')
                    {
                    if(from_session=='FN' && from_date==to_date && to_session=='FN')
                    {m=1;document.getElementById("apply").style.display='block';
                    document.getElementById("apply1").style.display='block';
                    }
                    else if(from_session=='both' && to_session=='both' && to_date>=from_date)
                    {m=2;document.getElementById("apply").style.display='block';
                    document.getElementById("apply1").style.display='block';
                    }
                    else if(from_session=='both' && to_session=='FN' && to_date>from_date)
                    {m=3;document.getElementById("apply").style.display='block';
                    document.getElementById("apply1").style.display='block';
                    }
                    else if(from_session=='AN' && to_session=='FN' && to_date>from_date)
                    {m=4;document.getElementById("apply").style.display='block';
                    document.getElementById("apply1").style.display='block';
                    }
                    else if(from_session=='AN' && to_session=='both' && to_date>from_date)
                    {m=5;document.getElementById("apply").style.display='block';
                    document.getElementById("apply1").style.display='block';
                    }
                    else if(from_session=='AN' && to_session=='AN' && to_date==from_date)
                    {m=6;document.getElementById("apply").style.display='block';
                    document.getElementById("apply1").style.display='block';
                    }
                    else
                    {
                    alert("Enter Valid Date/Session");
                    document.getElementById("apply").style.display='none';
                    document.getElementById("apply1").style.display='none';
                    }
                    }
                }
    
    
