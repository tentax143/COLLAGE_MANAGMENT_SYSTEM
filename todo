    <script>
        $(document).ready(function() {
            var minDate = new Date();
            var maxDate = new Date();
            maxDate.setDate(maxDate.getDate() + 7);
            minDate.setDate(minDate.getDate() - 1);

            $("#from_dateid_class").datepicker({
                showAnim: 'drop',
                numberOfMonth: 1,
                minDate: minDate,
                maxDate: maxDate,
                dateFormat: 'yy-mm-dd',
                onClose: function(selectedDate) {
                    $('#to_dateid_class').datepicker("option", "minDate", selectedDate);
                }
            });

            $("#to_dateid_class").datepicker({
                showAnim: 'drop',
                numberOfMonth: 1,
                dateFormat: 'yy-mm-dd'
            });
        });

        function updateDayOrder() {
            var fromDate = document.getElementById("from_dateid_class").value;
            var toDate = document.getElementById("to_dateid_class").value;

            if (fromDate && toDate) {
                var xmlhttp = new XMLHttpRequest();
                xmlhttp.open("GET", "get_day_order.php?from_date=" + fromDate + "&to_date=" + toDate, true);
                xmlhttp.send();
                xmlhttp.onreadystatechange = function() {
                    if (this.readyState == 4 && this.status == 200) {
                        var response = JSON.parse(this.responseText);
                        populateDayOrder(response);
                    }
                };
            }
        }

        function populateDayOrder(dayOrders) {
            var tbodyRef = document.getElementById('class_alter').getElementsByTagName('tbody')[0];
            tbodyRef.innerHTML = ''; // Clear existing rows

            dayOrders.forEach(function(dayOrder, index) {
                var newRow = tbodyRef.insertRow();
                var newCell = newRow.insertCell();
                var dateInput = document.createElement("INPUT");
                dateInput.setAttribute("type", "date");
                dateInput.setAttribute("name", "alt_date[]");
                dateInput.setAttribute("value", dayOrder.date);
                dateInput.setAttribute("required", '');
                newCell.appendChild(dateInput);

                newCell = newRow.insertCell();
                var classSelect = document.createElement("SELECT");
                classSelect.setAttribute("name", "alt_course[]");
                classSelect.setAttribute("required", '');
                // Populate class options dynamically
                newCell.appendChild(classSelect);

                newCell = newRow.insertCell();
                var hourSelect = document.createElement("SELECT");
                hourSelect.setAttribute("name", "alt_hour[]");
                hourSelect.setAttribute("required", '');
                // Populate hour options based on dayOrder.hours
                dayOrder.hours.forEach(function(hour) {
                    var option = document.createElement("option");
                    option.value = hour;
                    option.innerHTML = hour;
                    hourSelect.appendChild(option);
                });
                newCell.appendChild(hourSelect);

                newCell = newRow.insertCell();
                var facultySelect = document.createElement("SELECT");
                facultySelect.setAttribute("name", "alt_fac[]");
                facultySelect.setAttribute("required", '');
                // Populate faculty options dynamically
                newCell.appendChild(facultySelect);
            });
        }
    </script>
